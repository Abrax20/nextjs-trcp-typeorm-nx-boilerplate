  "scripts": {
    "test": "nx test",
    "start": "nx serve",
    "build": "nx build",
    "dev-next": "next dev",
    "audit": "improved-yarn-audit --ignore-dev-deps",
    "prod": "docker-compose -f docker-compose.yml -f docker-compose.local.yml",
    "dev": "docker-compose -f docker-compose.yml -f docker-compose.local.yml -f docker-compose.dev.yml",
    "typeorm": "ts-node --project ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "schema:drop": "yarn typeorm schema:drop -d libs/database/src/source.ts",
    "schema:sync": "yarn typeorm schema:sync -d libs/database/src/source.ts && yarn schema:sync:migrations",
    "schema:sync:migrations": "ts-node --project tsconfig.scripts.json -r tsconfig-paths/register ./scripts/sync.ts",
    "seed": "ts-node --project ./tsconfig.json -r tsconfig-paths/register ./scripts/seed.ts",
    "migration:run": "yarn typeorm migration:run -d libs/database/src/source.ts --transaction",
    "schema:diagram": "ts-node --project tsconfig.scripts.json -r tsconfig-paths/register ./scripts/generateDatabaseDiagram.ts",
    "migration:generate": "yarn typeorm migration:generate ./libs/database/src/migrations/migration -d ./libs/database/src/source.ts",
    "storybook:gen": "yarn nx g @nx/react:stories --project=app",
    "storybook:serve": "yarn nx run app:storybook"
  },
